# end of Kernel related setup

#
# BT debug and log system
#
CONFIG_BT_LOG_LEVEL=5
# end of BT debug and log system

#
# Crypto: TinyCrypt cryptography library
#
CONFIG_TINYCRYPT=y
# CONFIG_TINYCRYPT_CTR_PRNG is not set
CONFIG_TINYCRYPT_SHA256=y
CONFIG_TINYCRYPT_SHA256_HMAC=y
CONFIG_TINYCRYPT_SHA256_HMAC_PRNG=y
CONFIG_TINYCRYPT_ECC_DH=y
