#
# Board setup
#
CONFIG_ARM=y
CONFIG_PLATFORM_8710C=y
CONFIG_SYS_CLOCK_HW_CYCLES_PER_SEC=72000000
CONFIG_HEAP_MEM_POOL_SIZE=0
CONFIG_CLOCK_CONTROL=y
CONFIG_USE_ZEPHYR_BT_STACK=y
